
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  firstname String?
  lastname String?
  email String? @unique
  passwors String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  urls Urls[]
}

model Urls{
  id Int @id @default(autoincrement())
  shortUrl String?
  createrId  Int?
  longUrl String?
  createdAt DateTime @default(now())
  author User?   @relation(fields: [createrId], references: [id])
}